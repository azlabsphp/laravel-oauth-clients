{
    "name": "drewlabs/lv-oauth-clients",
    "description": "drewlabs/oauth-clients bindings for laravel framework",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "oauth",
        "clients"
    ],
    "autoload": {
        "psr-4": {
            "Drewlabs\\Laravel\\Oauth\\Clients\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\Laravel\\Oauth\\Clients\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "azandrewdevelopper@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "drewlabs/core-helpers": "^0.3.0",
        "drewlabs/oauth-clients": "^0.2.0|^0.3.0",
        "drewlabs/query-contracts": "^0.3.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\Laravel\\Oauth\\Clients\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0",
        "vlucas/phpdotenv": ">=3.0"
    }
}
